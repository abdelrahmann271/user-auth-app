{
  "name": "easy-gen-frontend",
  "version": "1.0.0",
  "description": "React frontend for Easy Gen App",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint src --ext .ts,.tsx",
